{
  "compatibilityJSON": "v3",
  "debug": true,
  "sort": false,
  "attr": {
    "list": [
      "data-i18n"
    ],
    "extensions": [
      ".html",
      ".htm"
    ]
  },
  "func": {
    "list": [
      "i18next.t",
      "i18n.t"
    ],
    "extensions": [
      ".js",
      ".jsx"
    ]
  },
  "trans": {
    "component": "Trans",
    "i18nKey": "i18nKey",
    "defaultsKey": "defaults",
    "extensions": [
      ".js",
      ".jsx"
    ],
    "fallbackKey": false,
    "supportBasicHtmlNodes": true,
    "keepBasicHtmlNodesFor": [
      "br",
      "strong",
      "i",
      "p"
    ]
  },
  "lngs": ["en"],
  "fallbackLng": "en",
  "ns": ["translation"],
  "defaultLng": "en",
  "defaultNs": "translation",
  "defaultValue": "",
  "resource": {
    "loadPath": "i18n/{{lng}}/{{ns}}.json",
    "savePath": "i18n/{{lng}}/{{ns}}.json",
    "jsonIndent": 2,
    "lineEnding": "\n"
  },
  "keySeparator": ".",
  "nsSeparator": ":",
  "context": true,
  "contextFallback": true,
  "contextSeparator": "_",
  "contextDefaultValues": [],
  "plural": true,
  "pluralFallback": true,
  "pluralSeparator": "_",
  "interpolation": {
    "prefix": "{{",
    "suffix": "}}"
  },
  "metadata": {},
  "allowDynamicKeys": false,
  "removeUnusedKeys": true
}
